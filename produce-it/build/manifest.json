{
  "name": "Produce-it",
  "description": "Chrome platform app.",
  "manifest_version": 2,
  "version": "0.2",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "background",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "alwaysOnTopWindows"
  ]
}
